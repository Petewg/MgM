/* MGM specific functions */

#include <hmg.ch>

/*
   mgm_WaitWindow( acMessage, lNoWait, aBackColor, aForeColor ) -> NIL
    - displays a wait waindow with more than 1 line if acMessage either contains EOLs or it's an array of strings
    - created by: Pete D.
    - version 1.0 - 2016-07-30
    - TODO: add (optional) icon & sound
*/
#define cFormName "__MGM_WAIT_WINDOW_"
FUNCTION mgm_WaitWindow( acMessage, lNoWait, aBackColor, aForeColor, lTone )
   // LOCAL cFormName := "_MGM_WAIT_WINDOW_"
   LOCAL nWidth, nHeight, aMsg, bInit, cMessage

   IF _IsWindowDefined( cFormName ) // Always release any pending wait win , if any
      DoMethod( cFormName, "Release" )
   ENDIF

   DoEvents()

   IF PCount() == 0 .OR. HB_ISNULL( acMessage )
      RETURN NIL
   ENDIF

   nWidth := 0

   IF HB_ISARRAY( acMessage )
      cMessage := ""
      Aeval( acMessage, {|e| cMessage := cMessage + e + hb_eol(), nWidth := Max( nWidth, GetTextWidth( , e ) ) } )
      cMessage := hb_StrShrink( cMessage, Len( hb_eol() ) )
      nHeight := ( Len( acMessage ) -1 ) * 18   
   ELSE
      aMsg := hb_ATokens( acMessage, .T. )
      AEval( aMsg, {|e| nWidth := Max( nWidth, GetTextWidth( , e ) )} )
      cMessage := acMessage
      nHeight := ( Len( aMsg ) -1 ) * 18   
   ENDIF

   IF HB_ISLOGICAL( lTone ) .AND. lTone
      bInit := {|| mgm_CenterWin2Win( GetFormHandle( cFormName ), _HMG_MainHandle ), Tone( 10000, 3 ),Tone( 10000, 2 )}
   ELSE
      bInit := {|| mgm_CenterWin2Win( GetFormHandle( cFormName ), _HMG_MainHandle )}
   ENDIF

   hb_default( @lNoWait, .F. )

   IF lNoWait
      DEFINE WINDOW &( cFormName ) CHILD TOPMOST ON INIT EVAL( bInit )
   ELSE
      DEFINE WINDOW &( cFormName ) MODAL ON INIT EVAL( bInit )
   ENDIF

   SetProperty( cFormName, "Width", Min( 1.2 * nWidth, Min( GetDesktopWidth(), 800 ) ) )
   SetProperty( cFormName, "Height", 36 + nHeight + GetBorderHeight() / 2 )
   SetProperty( cFormName, "Title", "" )
   SetProperty( cFormName, "TitleBar", .F. )
   SetProperty( cFormName, "SysMenu", .F. )

   IF mgm_IsArrayRGB( aBackColor )
      SetProperty( cFormName, "BackColor", aBackColor )
   ENDIF

   IF ! mgm_IsArrayRGB( aForeColor )
         aForeColor := Iif( mgm_IsArrayRGB( aBackColor ), mgm_InvertRGB( aBackColor ), {0,0,0,} )
   ENDIF

   @ Iif( IsVistaOrLater(), 4, 7 ), 12 LABEL lblMessage ;
      WIDTH GetProperty( cFormName, "Width" ) - 24 - GetBorderWidth() ;
      HEIGHT 18 + nHeight VALUE cMessage FONT "Segoe UI" SIZE 10 FONTCOLOR aForeColor TRANSPARENT CENTERALIGN

   END WINDOW

   DoMethod( cFormName, "Center" )

   _ActivateWindow( { cFormName }, .T. )

   IF ! lNoWait
      InkeyGUI( 0 )
      mgm_WaitWindow() // warning! recursion call!
   ENDIF

   RETURN NIL


FUNCTION mgm_IsArrayRGB ( aColor )
   IF HB_ISARRAY( aColor ) .AND. Len( aColor ) == 3
      RETURN ( ValType( aColor[1] ) == "N" .AND. ValType( aColor[2] ) == "N" .AND. ;
               ValType( aColor[3] ) == "N" )
   ENDIF

   RETURN .F.   

FUNCTION mgm_InvertRGB( anRGB )
   RETURN { 255-anRGB[1], 255-anRGB[2], 255-anRGB[3] }




